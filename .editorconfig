root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[*.{js,ts,jsx,tsx}]
quote_type = single
indent_size = 2

[*.json]
indent_size = 2

[*.yml]
indent_size = 2

[*.scss]
indent_size = 2

[*.html]
indent_size = 2
